{
  "colors.enable": true,
  "coc.source.highlight.enable": true,
  "suggest.autoTrigger": "none",
  "tsserver": {
    "enable": true,
    "lintOnSave": true,
    "filetypes": ["typescript", "typescriptreact", "javascript", "javascriptreact"]
  },
  "eslint": {
    "enable": true,
    "run": "onSave",
    "autoFixOnSave": true,
    "packageManager": "yarn",
    "filetypes": ["typescript", "typescriptreact", "javascript", "javascriptreact"]
  },
  "go.golangciLintCommand": "golangci-lint",
  "go.golangciLintFlags": ["run", "--fast"],
  "coc-prettier": {
    "enable": true,
    "filetypes": ["typescript", "javascript", "typescriptreact"]
  }
}

